{
  "name": "sportsgrit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^10.0.14",
    "@angular/localize": "^10.0.14",
    "@angular/material": "^10.0.2",
    "@angular/material-moment-adapter": "^9.2.4",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/platform-server": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-translate/core": "^10.0.2",
    "angular-font-awesome": "^3.1.2",
    "angularfire2": "^5.0.2",
    "chroma-js": "^1.4.1",
    "core-js": "^2.5.2",
    "exif-js": "^2.3.0",
    "firebase": "^9.21.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.22.2",
    "ngx-bootstrap": "^4.0.1",
    "ngx-editor": "^4.1.1",
    "ngx-google-places-autocomplete": "^2.0.1",
    "ngx-image-cropper": "^1.4.1",
    "ngx-mat-select-search": "3.3.0",
    "node-sass": "^4.14.1",
    "rxjs": "^6.5.5",
    "rxjs-tslint": "^0.1.8",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@babel/types": "^7.22.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^3.0.1",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "4.14.186",
    "@types/node": "14.6.2",
    "codelyzer": "^5.1.2",
    "hads": "^3.0.3",
    "htmlhint": "^1.1.4",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.1",
    "puppeteer": "^1.9.0",
    "stylelint": "^15.6.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.1",
    "ts-node": "^10.8.1",
    "tslint": "~5.11.0",
    "typescript": "^3.9.10"
  }
}
