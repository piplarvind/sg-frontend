{
  "name": "sportsgrit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@alyle/ui": "^1.9.3",
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^7.2.16",
    "@angular/material": "^7.2.16",
    "@angular/material-moment-adapter": "^6.4.7",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@ngx-translate/core": "^10.0.2",
    "angular-font-awesome": "^3.1.2",
    "angularfire2": "^5.0.2",
    "chroma-js": "^1.4.1",
    "core-js": "^2.5.2",
    "exif-js": "^2.3.0",
    "firebase": "^9.21.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.22.2",
    "ngx-bootstrap": "^3.0.1",
    "ngx-editor": "^4.1.1",
    "ngx-google-places-autocomplete": "^2.0.1",
    "ngx-image-cropper": "^1.4.1",
    "ngx-mat-select-search": "^1.4.1",
    "node-sass": "^4.14.1",
    "rxjs": "6.6.7",
    "rxjs-tslint": "^0.1.8",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.2.16",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^3.0.1",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "^4.14.50",
    "@types/node": "^10.12.0",
    "codelyzer": "^4.5.0",
    "hads": "^3.0.3",
    "htmlhint": "^1.1.4",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.1",
    "puppeteer": "^1.9.0",
    "stylelint": "^15.6.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.1",
    "ts-node": "^10.8.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.6"
  }
}
